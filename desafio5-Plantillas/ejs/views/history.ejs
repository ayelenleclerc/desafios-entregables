<h3 class="text-primary"> Productos </h3>
<br>
<% if (productos.length> 0) { %>
    <div class="table-responsive">
        <table class="table table-dark">
            <tr class="text-warning">
                <th class="font-weight-bold">Nombre</th>
                <th class="font-weight-bold">Precio</th>
                <th class="font-weight-bold">Imagen</th>
            </tr>
            <% productos.forEach(product=> { %>
                <tr>
                    <td>
                        <%=product.title%>
                    </td>
                    <td>
                        <%=product.price%>
                    </td>
                    <td>
                        <img src=<%=product.thumbnail%> style="width: 100px;">
                    </td>
                </tr>
                <% }); %>
        </table>
    </div>
    <% } else{ %>
        <h3 class="alert alert-danger">no se encontraron datos</h3>
        <% } %>